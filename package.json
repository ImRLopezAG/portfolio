{
	"name": "portfolio",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"start:prod": "turbo run start:prod",
		"dev": "turbo run dev",
		"dev:debug": "turbo run dev:debug",
		"lint": "turbo run lint",
		"clean": "$npm_execpath clean:workspaces && git clean -xdf node_modules .turbo",
		"clean:workspaces": "turbo run clean",
		"typecheck": "turbo run typecheck"
	},
	"devDependencies": {
		"turbo": "2.6.0",
		"typescript": "catalog:"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "bun@1.3.1",
	"workspaces": [
		"apps/*",
		"packages/*"
	],
	"catalog": {
		"typescript": "5.9.3",
		"zod": "4.0.0",
		"superjson": "2.2.2",
		"@types/bun": "1.3.0"
	},
	"catalogs": {}
}