{
	"name": "portfolio",
	"private": true,
	"scripts": {
		"build": "turbo run build",
		"start:prod": "turbo run start:prod",
		"dev": "turbo run dev",
		"dev:debug": "turbo run dev:debug",
		"lint": "turbo run lint",
		"clean": "$npm_execpath clean:workspaces && git clean -xdf node_modules .turbo",
		"clean:workspaces": "turbo run clean",
		"typecheck": "turbo run typecheck"
	},
	"devDependencies": {
		"turbo": "2.6.0",
		"typescript": "latest"
	},
	"engines": {
		"node": ">=22"
	},
	"packageManager": "bun@1.3.1",
	"workspaces": [
		"apps/*",
		"packages/*"
	]
}